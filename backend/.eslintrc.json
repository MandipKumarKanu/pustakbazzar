{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-console": "off",
    "consistent-return": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/*.test.js", "**/*.spec.js", "**/jest.config.js"]}]
  }
}
